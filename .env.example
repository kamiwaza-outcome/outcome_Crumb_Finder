# API Keys
SAM_API_KEY=your_sam_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Service Account
# Path to your Google service account JSON file
GOOGLE_SHEETS_CREDS_PATH=./service_account.json

